{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PoolMetadataListResponse",
  "type": "object",
  "required": [
    "metadatas"
  ],
  "properties": {
    "metadatas": {
      "type": "array",
      "items": {
        "type": "array",
        "items": [
          {
            "$ref": "#/definitions/UniquePoolId"
          },
          {
            "$ref": "#/definitions/PoolMetadata"
          }
        ],
        "maxItems": 2,
        "minItems": 2
      }
    }
  },
  "additionalProperties": false,
  "definitions": {
    "AssetEntry": {
      "description": "May key to retrieve information on an asset",
      "type": "string"
    },
    "PoolMetadata": {
      "type": "object",
      "required": [
        "assets",
        "dex",
        "pool_type"
      ],
      "properties": {
        "assets": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/AssetEntry"
          }
        },
        "dex": {
          "type": "string"
        },
        "pool_type": {
          "$ref": "#/definitions/PoolType"
        }
      }
    },
    "PoolType": {
      "type": "string",
      "enum": [
        "ConstantProduct",
        "Stable",
        "Weighted",
        "LiquidityBootstrap"
      ]
    },
    "UniquePoolId": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    }
  }
}
